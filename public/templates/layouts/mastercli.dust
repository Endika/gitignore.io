<html>
<head>
  <meta charset='utf-8'>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width">

  <title>{+title /}</title>

  <!-- Flatdoc -->
  <script src="/components/jquery/dist/jquery.min.js"></script>
  <script src='/components/flatdoc/legacy.js'></script>
  <script src='/components/flatdoc/flatdoc.js'></script>

  <!-- Flatdoc theme -->
  <link  href='/css/theme-gitignore-style.css' rel='stylesheet'>
  <script src='/js/theme-gitignore-script.js'></script>

  <!-- Meta -->
  <meta name="description" content="{+description /}">
  <meta name="keywords" content="gitignore,.gitignore,git,ignore,file,create,make,generate,build,automatic,programmatic,command line,cli">

  <meta property="og:type" content="website">
  <meta property="og:title" content="gitignore.io">
  <meta property="og:description" content="Create useful .gitignore files for your project">
  <meta property="og:image" content="https://gitignore.io/gitignore.png">
  <meta property="og:url" content="https://gitignore.io">

  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:site" content="@joe_blau">
  <meta name="twitter:title" content="gitignore.io">
  <meta name="twitter:description" content="Create useful .gitignore files for your project">
  <meta name="twitter:creator" content="@joe_blau">
  <meta name="twitter:image:src" content="https://gitignore.io/gitignore.png">
  <meta name="twitter:domain" content="https://gitignore.io">

  <meta itemprop="name" content="gitignore.io">
  <meta itemprop="description" content="Create useful .gitignore files for your project">
  <meta itemprop="image" content="https://gitignore.io/gitignore.png">

  <!-- Initializer -->
  <script>
    Flatdoc.run({
      fetcher: Flatdoc.file('/README.md')
    });
  </script>
</head>
<body role='flatdoc'>
  {+body /}
<script src="/components/retinajs/dist/retina.min.js"></script>
</body>
</html>
